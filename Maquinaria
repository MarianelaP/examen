package Examen1;
import javax.swing.JOptionPane;

public class Maquinaria {

    private String modelo;
    private String color;
    private String añoCompra;

    public Maquinaria(String modelo, String color, String añoCompra){
        this.modelo = modelo;
        this.color = color;
        this.añoCompra = añoCompra;

    }

    public String getModelo() {
        return modelo;
    }

    public void setModelo(String modelo) {
        this.modelo = modelo;
    }

    public String getColor() {
        return color;
    }

    public void seCcolor(String color) {
        this.color = color;
    }

    public String getAñoCompra() {
        return añoCompra;
    }

    public void setAñodecompra(String añodecompra) {
        this.añoCompra = añoCompra;

    }
}
